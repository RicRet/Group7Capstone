    "jest": "^29.7.0",
    "jest-expo": "~51.0.0",
    "react-test-renderer": "19.1.0",
    "supertest": "^7.1.1",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@types/supertest": "^2.0.16"
  }
